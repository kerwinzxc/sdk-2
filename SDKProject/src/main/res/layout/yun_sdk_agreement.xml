<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/yun_sdk_white"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/yun_sdk_rl_top"
        android:layout_width="match_parent"
        android:layout_height="50dip">

        <TextView
            android:id="@+id/yun_sdk_tv_back"
            android:layout_width="wrap_content"
            android:layout_height="38dp"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:padding="3dp"
            android:textSize="18sp"
            android:textColor="@color/yun_sdk_title"
            android:drawableLeft="@drawable/icon_arrow_left"
            android:drawableTint="@color/yun_sdk_black"
            android:drawablePadding="10dp"/>

        <TextView
            android:id="@+id/yun_sdk_tv_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:maxLength="8"
            android:hint="注册协议"
            android:textColor="@color/yun_sdk_black"
            android:textSize="20dp" />

        <ImageView
            android:id="@+id/yun_sdk_iv_cancel"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5dip"
            android:gravity="center"
            android:padding="5dp"
            android:src="@drawable/cancel"/>

    </RelativeLayout>

    <WebView
        android:id="@+id/webviewAgreement"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


</LinearLayout>
